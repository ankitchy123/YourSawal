	<script>
		const handleSubmit = async (e) => {
			e.preventDefault()
			const interests = []
			const x = e.target.interests.options;
			for (var i = 0; i < x.length; i++) {
				if (x[i].selected == true) {
					interests.push(x[i].value)
				}
			}

			const socialNetworks = {
				facebook: e.target.facebookurl.value,
				instagram: e.target.instagramurl.value,
				linkedin: e.target.linkedinurl.value,
				youtube: e.target.youtubeurl.value
			}

			console.log(e.target.firstname.value);
			const creds = { firstname: e.target.firstname.value, lastname: e.target.lastname.value, email: e.target.email.value, gender: e.target.gender.value, occupation: e.target.occupation.value, interests: interests, dob: e.target.dob.value, country: e.target.country.value, education: e.target.education.value, about: e.target.about.value, location: e.target.location.value, workExperience: e.target.workExperience.value, socialNetworks, hobbies: e.target.hobbies.value }

			const response = await fetch('http://localhost:3000/api/auth/update', {
				method: 'PATCH',
				headers: {
					'Content-Type': 'application/json'
				},
				body: JSON.stringify(creds)
			});
			const json = await response.json();

			if (json.success) {
				window.location.replace("http://localhost:3000/viewprofile");
			}
		}
		const logOut = async () => {
			const response = await fetch('http://localhost:3000/api/auth/logout', {
				method: 'GET',
				headers: {
					'Content-Type': 'application/json'
				},
			});
			const json = await response.json();
			if (json.success) {
				window.location.replace("http://localhost:3000/");
			}
		}

		const fetchdetails = async () => {
			const response = await fetch('http://localhost:3000/api/auth/getuser', {
				method: 'POST',
				headers: {
					'Content-Type': 'application/json'
				},
			});
			const json = await response.json();

			let name = document.getElementById('name')
			name.innerHTML = json.firstname + ' ' + json.lastname

			let sidename = document.getElementById('sidename')
			sidename.innerHTML = json.firstname + ' ' + json.lastname

			let sidefollowers = document.getElementById('sidefollowers')
			sidefollowers.innerHTML = json.followers.length

			let firstname = document.getElementById('firstname')
			firstname.value = json.firstname

			let lastname = document.getElementById('lastname')
			lastname.value = json.lastname

			let uppername = document.getElementById('uppername')
			uppername.innerHTML = json.firstname

			let mail = document.getElementById('email')
			mail.value = json.email
			let about = document.getElementById('about')
			about.value = json.about
			let occupation = document.getElementById('occupation')
			occupation.value = json.occupation
			let location = document.getElementById('location')
			location.innerHTML = json.location

			let locationinp = document.getElementById('locationinp')
			locationinp.value = json.location

			let education = document.getElementById('education')
			education.value = json.education
			let workExperience = document.getElementById('workExperience')
			workExperience.value = json.workExperience
			let hobbies = document.getElementById('hobbies')
			hobbies.value = json.hobbies

			let followers = document.getElementById('followers')
			followers.innerHTML = json.followers.length
			let following = document.getElementById('following')
			following.innerHTML = json.following.length

			let facebookurl = document.getElementById('facebookurl')
			facebookurl.value = json.socialNetworks.facebook
			let instagramurl = document.getElementById('instagramurl')
			instagramurl.value = json.socialNetworks.instagram
			let linkedinurl = document.getElementById('linkedinurl')
			linkedinurl.value = json.socialNetworks.linkedin
			let youtubeurl = document.getElementById('youtubeurl')
			youtubeurl.value = json.socialNetworks.youtube
		}
		fetchdetails()

		const handleSearch = async (e) => {
			e.preventDefault()
			const searchinput = document.getElementById('searchinput').value
			console.log(searchinput);
			let url = 'http://localhost:3000/api/auth/getuser/name/' + searchinput
			const user = await fetch(url, {
				method: 'GET'
			})
			const users = await user.json()
			if (users) {
				localStorage.setItem('searchResuls', JSON.stringify(users))
				console.log(users);
				window.location.replace("http://localhost:3000/users");
			}
		}
