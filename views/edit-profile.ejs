	<script>
		const handleSubmit = async (e) => {
			e.preventDefault()
			const interests = []
			const x = e.target.interests.options;
			for (var i = 0; i < x.length; i++) {
				if (x[i].selected == true) {
					interests.push(x[i].value)
				}
			}

			const creds = { firstname: e.target.firstname.value, lastname: e.target.lastname.value, email: e.target.email.value, gender: e.target.gender.value, occupation: e.target.occupation.value, interests: interests, dob: e.target.dob.value, country: e.target.country.value }

			const response = await fetch('http://localhost:3000/api/auth/update', {
				method: 'PATCH',
				headers: {
					'Content-Type': 'application/json'
				},
				body: JSON.stringify(creds)
			});
			const json = await response.json();
			// console.log(json);
			if (json.success) {
				window.location.replace("http://localhost:3000/viewprofile");
			}
		}
		const logOut = async () => {
			const response = await fetch('http://localhost:3000/api/auth/logout', {
				method: 'GET',
				headers: {
					'Content-Type': 'application/json'
				},
			});
			const json = await response.json();
			if (json.success) {
				window.location.replace("http://localhost:3000/");
			}
		}


		const fetchdetails = async () => {
			const response = await fetch('http://localhost:3000/api/auth/getuser', {
				method: 'POST',
				headers: {
					'Content-Type': 'application/json'
				},
			});
			const json = await response.json();
			console.log(json);
			let mail = document.getElementById('email')
			mail.value = json.email
			let about = document.getElementById('about')
			about.value = json.about
			let occupation = document.getElementById('occupation')
			occupation.value = json.occupation
			let location = document.getElementById('location')
			location.value = json.location

			let interests = document.getElementById('interests')
			interests.value = json.interests
		}
		fetchdetails()
	</script>


